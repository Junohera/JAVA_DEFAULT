	public EMSInformationService() {
		
	}
	
	public void outputEMSInformation() {
		System.out.print("\n\n");
		String devPeriod = 	sYear 	+"/"+
							sMonth	+"/"+
							sDay	+"/"+
							eYear	+"/"+
							eMonth	+"/"+
							eDay;
							
							
	}
	
}
